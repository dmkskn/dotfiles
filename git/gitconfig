[user]
	name = Dima Koskin
	email = dmkskn@dmkskn.com

[core]
    excludesfile = ~/.gitignore
	editor = code -n -w

[alias]
    s = status
    ss = status -s
    l = log
    ll = log --oneline
    uncommit = reset HEAD^

[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
